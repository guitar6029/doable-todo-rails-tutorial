<h1>New Project</h1>

<%= form_with modle: @project do |f|%>
    <div class="field">
            <%= f.label :name %>
            <%= f.text_field :name %>
    </div>
    
    <div class="actions">
        <%= f.submit "Create Project"%>
    </div>

<% end %>

<form action="/Projects" method="post">
    <input type="text" name="project[name]" />
    <input type="submit" value="Create Project" />
</form>